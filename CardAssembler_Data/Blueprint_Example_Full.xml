<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE data>
<!-- spell-checker: disable -->

<!-- CardID: "unique spell example". -->

<data>
    <unique>
        <spell>
            <example>
                <next>template spell layout</next>
                <command01_image>
                    <name>Fluff</name>
                </command01_image>
                <command02_background>
                    <next>color spell fast</next>
                </command02_background>
                <command04_name>
                    <text>Fluff</text>
                </command04_name>
                <command05_text>
                    <text>Pay one mana.\nTo the end of the turn, increase cuteness by one of two selected kittens.</text>
                </command05_text>
                <command08_symbol1>
                    <next>template spell symbol kitty</next>
                    <next>template spell symbol position p1</next>
                </command08_symbol1>
                <command09_symbol2>
                    <next>template spell symbol kitty</next>
                    <next>template spell symbol position p2</next>
                </command09_symbol2>
                <command10_symbol3>
                    <next>template spell symbol text</next>
                    <next>template spell symbol position p3</next>
                    <text>+1</text>
                </command10_symbol3>
            </example>
            <anotherSpell></anotherSpell>
        </spell>
        <item></item>
        <monster></monster>
    </unique>
    <template>
        <textScale>
            <goldenRatio>
                <textScale parse="float">1.6</textScale>
            </goldenRatio>
            <goldenRatioSquared>
                <textScale parse="float">2.6</textScale>
            </goldenRatioSquared>
        </textScale>
        <textSize>
            <fontSize parse="int">40</fontSize>
        </textSize>
        <spell>
            <layout>
                <command00_loadDataImage>
                    <commandType>import_layer_load</commandType>
                    <filename>Data_image_Example.xcf</filename>
                    <name>dataImage1</name>
                </command00_loadDataImage>
                <command01_image>
                    <next>template spell size</next>
                    <commandType>image</commandType>
                </command01_image>
                <command02_background>
                    <next>template spell size</next>
                    <commandType>monochrome</commandType>
                    <name>Background</name>
                </command02_background>
                <command03_loadFrame>
                    <commandType>import_layer</commandType>
                    <targetFile>dataImage1</targetFile>
                    <targetLayer>Corner</targetLayer>
                </command03_loadFrame>
                <command04_name>
                    <next>color black</next>
                    <next>template textSize</next>
                    <next>template textScale goldenRatio</next>
                    <commandType>text</commandType>
                    <name>Name</name>
                    <font>Tahoma Bold</font>
                    <size parse="tuple">400, 100</size>
                    <lineSpacing parse="float">0</lineSpacing>
                    <letterSpacing parse="float">0.8</letterSpacing>
                    <justification parse="int">2</justification>
                    <position parse="tuple">36, 60</position>
                </command04_name>
                <command05_text>
                    <next>template textSize</next>
                    <next>color black</next>
                    <commandType>text</commandType>
                    <name>Text</name>
                    <font>Cambria</font>
                    <fontSize parse="int">20</fontSize>
                    <size parse="tuple">350, 200</size>
                    <lineSpacing parse="float">8.5</lineSpacing>
                    <letterSpacing parse="float">-1.4</letterSpacing>
                    <position parse="tuple">100, 200</position>
                </command05_text>
                <command06_stripe>
                    <next>color white</next>
                    <commandType>monochrome</commandType>
                    <name>Stripe</name>
                    <position parse="tuple">50, 00</position>
                    <size parse="tuple">25, 250</size>
                </command06_stripe>
                <command07_symbolsGroup>
                    <commandType>group</commandType>
                    <name>Symbols</name>
                </command07_symbolsGroup>
            </layout>
            <size>
                <size parse="tuple">450, 450</size>
            </size>
            <symbol>
                <kitty>
                    <commandType>import_layer</commandType>
                    <targetFile>dataImage1</targetFile>
                    <addToPosition parse="int">-1</addToPosition>
                    <targetLayer>Kitty</targetLayer>
                </kitty>
                <text>
                    <next>color white</next>
                    <next>template textSize</next>
                    <next>template textScale goldenRatio</next>
                    <commandType>text</commandType>
                    <addToPosition parse="int">-1</addToPosition>
                    <font>Arial</font>
                    <size parse="tuple">100, 97</size>
                    <justification parse="int">2</justification>
                </text>
                <position>
                    <p1>
                        <position parse="tuple">30, 350</position>
                    </p1>
                    <p2>
                        <position parse="tuple">130, 350</position>
                    </p2>
                    <p3>
                        <position parse="tuple">230, 350</position>
                    </p3>
                </position>
            </symbol>
        </spell>
    </template>
    <color>
        <white>
            <color>#ffffff</color>
        </white>
        <black>
            <color>#000000</color>
        </black>
        <spell>
            <fast>
                <color>#c68500</color>
            </fast>
        </spell>
    </color>
